{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio",
  "main": "index.js",
  "scripts": {
    "build": "npm run write:html && npm run build:client",
    "build:client": "react-scripts build",
    "eslint": "eslint \"src/*/**\" \"utils/*/**\"",
    "dev": "npm run prepopulate && react-scripts start",
    "prepopulate": "node --experimental-modules utils/index.js",
    "write:html": "ts-node -T -P configs/typescript/tsconfig.utils.json utils/scripts/write-html.ts"
  },
  "keywords": [
    "portfolio"
  ],
  "type": "module",
  "author": "Kevin Han",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mongoose": "^5.8.11",
    "prettier": "^1.19.1",
    "react-scripts": "^3.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
