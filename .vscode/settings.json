{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "cSpell.ignorePaths": [
    "**/node_modules/**",
    "**/.git/objects/**",
    ".env",
    "yarn.lock"
  ],
  "cSpell.ignoreWords": [
    "Contentful",
    "contentfulchildren",
    "DDTHH",
    "gridlines",
    "iPhone",
    "Macbook",
    "mixins",
    "Muli",
    "noreferrer",
    "noopener",
    "SSSZ",
    "stylelint",
    "truetype",
    "viewports",
    "webp"
  ]
}
